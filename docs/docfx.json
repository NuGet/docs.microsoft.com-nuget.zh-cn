{
  "build": {
    "markdownEngineName": "markdig",
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "exclude": [
          "**/obj/**",
          "nuget/**",
          "**/includes/**",
          "archive/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg",
          "**/*.json",
          "**/*.xml"
        ],
        "exclude": [
          "**/obj/**",
          "nuget/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
        "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "NuGet/docs.microsoft.com-nuget.zh-cn",
      "feedback_product_url": "https://github.com/NuGet/Home/issues/",
            "breadcrumb_path": "~/_breadcrumb/toc.yml",
            "ROBOTS": "INDEX,FOLLOW",
            "searchScope": ["NuGet"],
            "uhfHeaderId": "MSDocsHeader-NuGet"
            },
    "fileMetadata": {},
    "template": [],
    "dest": "nuget"
  }
}
